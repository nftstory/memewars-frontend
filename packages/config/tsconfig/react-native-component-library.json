{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "React Native Component Library",
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "allowJs": true,
        "strict": true,
        "jsx": "react-native",
        "lib": [
            "DOM",
            "ESNext"
        ],
        "noEmit": true,
        "target": "ESNext"
    }
}